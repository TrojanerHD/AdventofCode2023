  "version": "0.2.0",
  "configurations": [
    {
      "command": "bun run ${workspaceFolder}/src/index.ts 1",
      "request": "launch",
      "name": "Bun",
      "type": "node-terminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Node",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/build/src/index.js",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": ["${workspaceFolder}/build/**/*.js"],
      "args": ["1"]
    }
  ]
}